{
    "name": "aheissenberger/wordpress-message-repository",
    "type": "library",
    "description": "Wordpress Message Repository for EventSauce",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Heissenberger",
            "email": "andreas@heissenberger.at"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "eventsauce/eventsauce": "^0.8.1",
        "ext-json": "^7.4",
        "ramsey/uuid": "^4.1",
        "phpunit/phpunit": "^9.4"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "EventSauce\\WordpressMessageRepository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EventSauce\\WordpressMessageRepository\\Tests\\": "tests"
        }
    },
    "archive": {
        "exclude": ["/tests", ".devcontainer", ".vscode"]
    }
}
